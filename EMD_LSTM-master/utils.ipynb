   "source": [
    "import datetime as dt\n",
    "\n",
    "class Timer():\n",
    "    def __init__(self):\n",
    "        self.start_dt = None\n",
    "        \n",
    "    def start(self):\n",
    "        self.start_dt = dt.datetime.now()\n",
    "        \n",
    "    def stop(self):\n",
    "        end_dt = dt.datetime.now()\n",
    "        print('Time taken: %s' % (end_dt - self.start_dt))"
   ]
  }
